{
	"name": "@jgchk/trpc-svelte-query",
	"version": "1.3.2",
	"description": "tRPC Svelte lib",
	"author": "jgchk",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"typesVersions": {
		"*": {
			"*": [
				"dist/index.d.ts"
			],
			"shared": [
				"dist/shared/index.d.ts"
			]
		}
	},
	"homepage": "https://trpc.io",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/trpc/trpc.git",
		"directory": "packages/svelte-query"
	},
	"scripts": {
		"build": "rm -r dist || true && tsup",
		"dev": "tsup --watch",
		"lint": "tsc --noEmit && eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives src"
	},
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./shared": {
			"import": "./dist/shared/index.mjs",
			"require": "./dist/shared/index.js",
			"default": "./dist/shared/index.js"
		}
	},
	"files": [
		"dist",
		"src",
		"README.md",
		"shared"
	],
	"eslintConfig": {
		"rules": {
			"no-restricted-imports": [
				"error",
				"@jgchk/trpc-svelte-query"
			]
		}
	},
	"peerDependencies": {
		"@tanstack/svelte-query": "^4.26.0",
		"@trpc/client": "^10.16.0",
		"@trpc/server": "^10.16.0",
		"svelte": "^3.55.1"
	},
	"devDependencies": {
		"@tanstack/svelte-query": "^4.27.0",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@trpc/client": "^10.18.0",
		"@trpc/server": "^10.18.0",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"@typescript-eslint/parser": "^5.57.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unicorn": "^46.0.0",
		"prettier": "^2.8.7",
		"svelte": "^3.57.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"esm-env": "^1.0.0"
	}
}
